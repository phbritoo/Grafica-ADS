<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee

http://xmlns.jcp.org/xml/ns/javaee/web-
facesconfig_2_2.xsd"

	version="2.2">

	<navigation-rule>
		<from-view-id>/xhtml/cadastroFuncionario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inserido</from-outcome>
			<to-view-id>/xhtml/listarFuncionarios.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/xhtml/cadastroCliente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inserido</from-outcome>
			<to-view-id>/xhtml/listarClientes.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/xhtml/cadastroOS.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inserido</from-outcome>
			<to-view-id>/xhtml/listarServicos.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/xhtml/listarClientes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ecluido</from-outcome>
			<to-view-id>/xhtml/listarClientes.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/xhtml/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>entrar</from-outcome>
			<to-view-id>/xhtml/listarFuncionarios.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
</faces-config>

